{
  "name": "entrevistas",
  "version": "0.1.0",
  "private": true,
  "author": "Carlos Mahonri Monterroso Garcia",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:security": "eslint --config .eslintrc.security.mjs src/",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:check": "pnpm run security:audit && pnpm run lint:security",
    "security:full": "pnpm run security:check",
    "security:deps": "pnpm audit --audit-level moderate && pnpm outdated"
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
